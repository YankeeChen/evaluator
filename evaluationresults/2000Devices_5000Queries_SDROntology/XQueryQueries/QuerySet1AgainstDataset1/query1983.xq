  let $GRAPH_7 := ($GRAPH_5,$GRAPH_6)
  let $GRAPH_8 := ($GRAPH_3,$GRAPH_7)
  let $GRAPH_9 := xqllib:and($GRAPH_1,$GRAPH_8,("Entity_0"))
  let $GRAPH_10 := xqllib:and($GRAPH_0,$GRAPH_9,("SearchRadar_0"))
