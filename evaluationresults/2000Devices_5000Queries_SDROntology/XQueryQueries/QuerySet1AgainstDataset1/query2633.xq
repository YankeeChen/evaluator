  let $GRAPH_4 :=  
    for $node_Quantity_0 in $doc_ObjectDescription
    let $value_Quantity_0 := fn:data($node_Quantity_0/@rdf:about)
    for $node_TerrainBounceJamming_0 in $node_Quantity_0/RFDevice:hasEffectiveRadiatedPower
    let $value_TerrainBounceJamming_0 := fn:data($node_TerrainBounceJamming_0/@rdf:resource)
    where fn:exists($node_Quantity_0) and fn:exists($value_TerrainBounceJamming_0)
