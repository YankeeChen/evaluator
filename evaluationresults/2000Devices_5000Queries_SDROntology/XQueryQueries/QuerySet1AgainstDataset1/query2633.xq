  let $GRAPH_6 := xqllib:and($GRAPH_4,$GRAPH_5,("Quantity_0"))
  let $GRAPH_7 := xqllib:and($GRAPH_3,$GRAPH_6,("TerrainBounceJamming_0"))
  let $GRAPH_8 := xqllib:and($GRAPH_2,$GRAPH_7,("Jammer_0"))
  let $GRAPH_9 := xqllib:and($GRAPH_1,$GRAPH_8,("Jammer_0"))
  let $GRAPH_10 := xqllib:optional($GRAPH_0,$GRAPH_9,("MonostaticRadar_0"))
