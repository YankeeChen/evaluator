    for $node_Process_0 in $node_Object_1/Nuvio:subprocessOf
    let $value_Process_0 := fn:data($node_Process_0/@rdf:resource)
    where fn:exists($node_Object_1) and fn:exists($value_Process_0)
    return
      <xqllib:result>
