    for $node_Entity_0 in $node_FreqencyRange_0/RFDevice:hasBandwidthRange
    let $value_Entity_0 := fn:data($node_Entity_0/@rdf:resource)
    where fn:exists($value_Entity_0) and fn:exists($node_FreqencyRange_0)
    return
