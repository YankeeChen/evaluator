    where fn:exists($node_Entity_0) and fn:exists($node_Quality_0) and $node_Quality_0/Nuvio:hasProcessQuantity/@rdf:resource = $value_Entity_0
    return
      <xqllib:result>
        <xqllib:var name="Entity_0">{$value_Entity_0}</xqllib:var>
        <xqllib:var name="Quality_0">{$value_Quality_0}</xqllib:var>
