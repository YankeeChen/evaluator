  let $GRAPH_6 := xqllib:and($GRAPH_4,$GRAPH_5,("Quality_0"))
  let $GRAPH_7 := xqllib:and($GRAPH_3,$GRAPH_6,("Entity_0"))
  let $GRAPH_8 := xqllib:and($GRAPH_2,$GRAPH_7,("Entity_0"))
